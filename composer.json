{
  "name": "trogers1884/laravel-schedule-mgt",
  "description": "A Laravel package for managing scheduled tasks through file storage",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Rogers",
      "email": "trogers1884@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Trogers1884\\LaravelScheduleMgt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trogers1884\\LaravelScheduleMgt\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Trogers1884\\LaravelScheduleMgt\\ScheduleMgtServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}